<Window x:Class="TTMouseclickSimulator.PredefinedProjectDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TTMouseclickSimulator"
        mc:Ignorable="d"
        Title="Predefined Project" Height="300" Width="300" TextOptions.TextFormattingMode="Display" ShowInTaskbar="False">
    <Grid>
        <ListBox x:Name="listBox" Margin="10,10,10,35"/>
        <Button x:Name="btnOk" IsDefault="True" Content="OK" Margin="0,0,101,8" HorizontalAlignment="Right" Width="75" Height="19" VerticalAlignment="Bottom" Click="btnOk_Click"/>
        <Button IsCancel="True" Content="Cancel" Margin="0,0,10,8" HorizontalAlignment="Right" Width="75" Height="19" VerticalAlignment="Bottom"/>

    </Grid>
</Window>
